<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dashboard">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dashboard"
        tools:targetApi="31">
        <activity
            android:exported="false"
            android:name="com.example.user.Signup" />
        <activity
            android:exported="false"
            android:name="com.example.user.Login" />
        <activity
            android:exported="false"
            android:name="com.example.gamelist.GamesActivity" />
        <activity
            android:exported="false"
            android:name="com.example.gamelist.GameDetailsActivity" />
        <activity
            android:exported="false"
            android:name="com.example.dashboard.Activity.TopBoostersActivity" />
        <activity
            android:exported="false"
            android:name="com.example.dashboard.Activity.DetailActivity" />
        <activity
            android:exported="false"
            android:name="com.example.dashboard.Activity.BaseActivity" />
        <activity
            android:exported="false"
            android:name="com.example.dashboard.Activity.MainActivity" />
        <activity
            android:exported="false"
            android:name="com.example.dashboard.Activity.BoostRequestActivity" />
        <activity
            android:exported="true"
            android:name="com.example.dashboard.Activity.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:exported="false"
            android:name="com.example.user.ForgotPasswordActivity" />
    </application>

</manifest>